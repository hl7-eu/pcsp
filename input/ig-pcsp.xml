<?xml version="1.0" encoding="UTF-8"?>
<!-- Start by finding all references to "myig" and updating to appropriate text for your IG, including changing realm -->
<ImplementationGuide xmlns="http://hl7.org/fhir"
                   	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<id value="hl7.eu.fhir.pscp"/>
	<!--  <extension url="http://hl7.org/fhir/tools/StructureDefinition/igpublisher-spreadsheet">
    <valueString value="resources-spreadsheet.xml"/>
  </extension>-->
	<url value="http://hl7.eu/fhir/ig/pcsp/ImplementationGuide/hl7.eu.fhir.pscp"/>
	<!-- This version will propagate to all artifacts unless the "propagate-version" extension is overridden -->
	<version value="0.0.1"/>
	<name value="PCSPImplementationGuide"/>
	<title value="PanCareSurPass FHIR Implementation Guide"/>
	<status value="draft"/>
	<experimental value="false"/>
	<publisher value="PanCareSurPass Project"/>
	<contact>
		<telecom>
			<!-- Or whatever URL and/or email address(es) are appropriate -->
			<system value="url"/>
			<value value="http://www.x-eHealth-project.eu"/>
		</telecom>
	</contact>
	<description value="PanCareSurPass is an EU project aiming to conduct a robust assessment of the Cancer Survivorship Passport (SurPass)."/>
	<jurisdiction>
		<!-- This will drive SNOMED release used -->
		<coding>
			<!-- This is the code for universal -->
			<system value="http://unstats.un.org/unsd/methods/m49/m49.htm"/>
			<code value="150"/>
			<!-- Change to this if the IG is country-specific
      <system value="http://unstats.un.org/unsd/methods/m49/m49.htm"/>
      <code value="001"/>
      -->
		</coding>
	</jurisdiction>
	<packageId value="hl7.eu.fhir.pscp"/>
	<!-- This should be changed to 'not-open-source' or another license if appropriate for non-HL7-published content -->
	<license value="CC0-1.0"/>
	<!-- This is whatever FHIR version(s) the IG artifacts are targeting (not the version of this file, which should always be 'current release') -->
	<fhirVersion value="4.0.1"/>
	<dependsOn>
		<!-- <uri value="http://hl7.org/fhir/uv/ips/ImplementationGuide/ig-uv-ips"/> -->
		<uri value="http://hl7.org/fhir/uv/ips/ImplementationGuide/hl7.fhir.uv.ips"/>
		<packageId value="hl7.fhir.uv.ips"/>
		<version value="current"/>
	</dependsOn>
	<definition>
		<!-- 		<resource>
			<reference>
				<reference value="Patient/pat-1"/>
			</reference>
			<name value="Patient: patient example"/>
			<description value="Patient: patient example"/>
			<exampleCanonical value="http://hl7.eu/fhir/ig/xeh/StructureDefinition/Patient-eu-gk"/>
		</resource> -->
		<!-- You don't need to define any groupings.  The IGPublisher will define them for you.  You only need to do so if your IG is 'special' and it's
      inappropriate to use the defaults.  Feel free to provide feedback about the defaults... -->
		<page>
			<!-- The root will always be toc.html - the template will force it if you don't do it -->
			<nameUrl value="toc.html"/>
			<title value="Table of Contents"/>
			<generation value="html"/>
			<page>
				<nameUrl value="index.html"/>
				<title value="PanCareSurPass IG Home Page"/>
				<generation value="markdown"/>
			</page>
			<page>
				<nameUrl value="logicalModel.html"/>
				<title value="Logical Models"/>
				<generation value="markdown"/>
			</page>
			<page>
				<nameUrl value="sequence.html"/>
				<title value="Sequence diagrams"/>
				<generation value="html"/>
			</page>
			<page>
				<nameUrl value="collaboration.html"/>
				<title value="Collaboration diagrams"/>
				<generation value="markdown"/>
			</page>
			<page>
				<nameUrl value="downloads.html"/>
				<title value="Useful Downloads"/>
				<generation value="html"/>
			</page>
			<page>
				<nameUrl value="changes.html"/>
				<title value="IG Change History"/>
				<generation value="html"/>
			</page>
		</page>
		<!-- copyright year is a mandatory parameter -->
		<parameter>
			<code value="copyrightyear"/>
			<value value="2021+"/>
		</parameter>
		<!-- releaselabel should be the ballot status for HL7-published IGs. -->
		<parameter>
			<code value="releaselabel"/>
			<value value="CI Build"/>
		</parameter>
		<parameter>
			<code value="find-other-resources"/>
			<value value="true"/>
		</parameter>
		<!--
		<parameter>
			<code value="logging"/>
			<value value="generate"/>
		</parameter>
-->
		<!-- Uncomment one or more of these if you want to limit which syntaxes are supported or want to disable the display of mappings
    <parameter>
